[ignore]
.*/node_modules/.*

[include]

[libs]
./node_modules/@teamcity/react-api
./node_modules/@reduxjs/toolkit/dist/export.flow.js

[options]
esproposal.optional_chaining=enable
module.name_mapper.extension='svg' -> '<PROJECT_ROOT>/SVGFlowStub.js.flow'

[strict]
nonstrict-import
unclear-type
unsafe-getters-setters
untyped-import
untyped-type-import
sketchy-null
implicit-inexact-object
